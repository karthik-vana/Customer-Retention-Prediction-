{
    "version": 2,
    "builds": [
        {
            "src": "app/app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "models/**",
                    "scalers/**",
                    "app/templates/**",
                    "app/static/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/app.py"
        }
    ]
}